    "react-dom": "16.12.0",
    "react-fast-compare": "2.0.4",
    "react-interval-timer": "1.0.5",
    "react-onclickoutside": "6.9.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-select": "3.0.8",
    "react-table": "6.11.5",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0"
